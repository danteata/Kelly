=semantic_form_for(@intake, :html => { :multipart => true }, :validate => true) do |f| 
  - if @intake.errors.any?
    .error_messages
      %ul
        - for message in @intake.errors.full_messages
          %li
            = message
  = f.inputs do
    %br
    .field
    = f.input :employee_token,:label => "Employee Name"
    %br
    .field
    = f.input :equipment_token,:label => "Equipment"
    %br
    .field
    = f.input :fuel_token,:label => "Fuel Type"
    %br
    .field
      = f.input :quantity
    %br
    .field
      = f.input :location
    %br
    .field
      = f.input :distance_covered
    %br
    .field
      = f.input :price_per_litre
    %br
    .field
      = f.buttons 


